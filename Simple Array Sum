#include<stdio.h>
int simpleArraySum(int array[], int n){
    int sum=0;
    for(int i=0;i<n;i++){
        sum+=array[i];
    }
    return sum;
}

int main(){
    int n;
    printf("Enter the number of elements: ");
    scanf("%d",&n);
    int array[n];
    printf("Enter values: ");
    for(int i=0;i<n;i++){
        scanf("%d",&array[i]);
    }
    printf("The sum of elements is %d",simpleArraySum(array,n));
}
